@model string

@{
    ViewData["Title"] = "Home Page";
}

<section class="mx-auto max-w-3xl">
    <div class="text-center space-y-4">
        <h1 class="text-3xl sm:text-4xl font-bold tracking-tight">Welcome to NRL Registration System</h1>
        

        @if (!string.IsNullOrWhiteSpace(Model))
        {
            <p class="text-gray-800">@Model</p>
        }

        <div>
            <a asp-action="DataForm" asp-controller="Obstacle"
               class="inline-flex items-center rounded-lg px-4 py-2 ring-1 ring-indigo-600 text-indigo-700 hover:bg-indigo-50">
                Register Obstacle
            </a>
        </div>
    </div>
</section>


